{
    "schemaVersion": "1.0.0",
    "class": "Device",
    "async": true,
    "label": "my BIG-IP declaration for declarative onboarding",
    "Common": {
        "class": "Tenant",
        "hostname": "bigip1.cactuslizard.com",
        "myLicense": {
			"class": "License",
			"licenseType": "regKey",
			"regKey": "VMHGU-RLMMW-PBPBA-ESVBQ-CURWPFS"
		},
        "myDns": {
            "class": "DNS",
            "nameServers": [
                "169.254.169.254",
				"8.8.8.8"
            ]
        },		
        "myNtp": {
            "class": "NTP",
            "servers": [
                "0.pool.ntp.org",
                "1.pool.ntp.org",
                "2.pool.ntp.org"				
            ],
            "timezone": "UTC"
        },
        "admin": {
            "class": "User",
            "userType": "regular",
            "password": "{{{ ADMIN_PASS }}}",
            "keys": ["{{{ SSH_KEYS }}}"],
            "shell": "bash"
        },
        "{{{ USER_NAME }}}": {
            "class": "User",
            "userType": "regular",
            "password": "{{{ ADMIN_PASS }}}",
            "keys": ["{{{ SSH_KEYS }}}"],
            "shell": "bash",
            "partitionAccess": {
                "all-partitions": {
                    "role": "admin"
                }
            }
        }
    }
}